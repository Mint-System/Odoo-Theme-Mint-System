<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template id="customstyle" name="Custom style" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/css" href="/theme_mint_system/static/css/style.css"/>
            <script type="text/javascript" src="/theme_mint_system/static/js/main.js"/>
        </xpath>
    </template>


    <template id="summernote" name="summernote overrides" inherit_id="website.assets_editor">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/theme_mint_system/static/js/summernote_overrides.js" />
        </xpath>
    </template>

</odoo>
